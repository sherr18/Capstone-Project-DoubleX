	<body>
		<!--头部开始	-->
		<header class="header">
			<div class="container">
				<div class="header_area">
					<div class="logo">
						<a href="index.html">
							<img src="picture/logo.png" alt="logo"/>
						</a>
					</div>	
					<div class="meau">
						<ul>
							<li>
								<a href="/">Home</a>
							</li>
							<li style="width:240px;">
								<a href="/popularList">Popular Recommended Movies</a>
							</li>
							<li id="login">
								<a class="" href="/login">Login</a>
							</li>
							<li class="meau_search">
								<input type="text" id="texts">
								<button onclick="search()">
