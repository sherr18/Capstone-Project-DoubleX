						</ul>
					</div>
				</div>
			</div>
			<!--头部结束-->			
		</header>
		<!--背景开始-->
		<!-- breadcrumb area start -->
		<section class="breadcrumb-area" style="background-image: url('https://image.tmdb.org/t/p/original<%=movie.backdrop_path%>');">
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
						<div class="breadcrumb-area-content">
							<h1><%=movie.original_title%></h1>
						</div>
					</div>
				</div>
			</div>
		</section><!-- breadcrumb area end -->
		<!-- transformers area start -->
		<section class="transformers-area">
			<div class="container">
				<div class="transformers-box">
					<div class="row flexbox-center">
						<div class="col-lg-5 text-lg-left text-center">
							<div class="transformers-content">
								<img style="width: 21vw;" src="https://image.tmdb.org/t/p/original<%=movie.poster_path%>" alt="about" />
							</div>
						</div>
						<div class="col-lg-7" style="margin-left: 50px;">
							<div class="transformers-content mtr-30">
								<h2><%=movie.original_title%></h2>
								<% if(movie.genres!=''){ %>
                                    <% for (var ge in movie.genres) { %>
										<a href="#"><%=movie.genres[ge].name%></a> | 
									<% } %>
                                <% } %>								
								<ul>
									<% if(movie.production_companies!=''){ %>
										<li>
											<div class="transformers-left">
												Production Companies:
											</div>
											<div class="transformers-right">
												<% for (var pcm in movie.production_companies) { %>
													<%=movie.production_companies[pcm].name%>  /
												<% } %>
											</div>
										</li>
									<% } %>
									
									<% if(movie.production_countries!=''){ %>
										<li>
											<div class="transformers-left">
												Production Countries:
											</div>
											<div class="transformers-right" style="margin-left: 1vw;">
												<% for (var pces in movie.production_countries) { %>
													<%=movie.production_countries[pces].name%>  /
												<% } %>
											</div>
										</li>
									<% } %>

									<% if(movie.director!=''){ %>
										<li>
											<div class="transformers-left">
												Directing:
											</div>
											<div class="transformers-right">
												<%=movie.director.name%>
											</div>
										</li>
									<% } %>

									<li>
										<div class="transformers-left">
											Release Date:
										</div>
										<div class="transformers-right">
											<%=movie.release_date%>
										</div>
									</li>

									<% if(movie.runtime!=''){ %>
									<li>
										<div class="transformers-left">
											RuntTme: 
										</div>
										<div class="transformers-right">
											<%=movie.runtime%> Min
										</div>
									</li>
									<% } %>
									
									<li>
										<div class="transformers-left">
											Popularity: 
										</div>
										<div class="transformers-right">
											<%=movie.popularity%>
										</div>
									</li>
									<li>
										<div class="transformers-left">
											Language:
										</div>
										<div class="transformers-right">
											<%=movie.original_language%>
										</div>
									</li>
									
